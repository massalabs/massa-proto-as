{
  "name": "massa-proto-as",
  "version": "0.0.1",
  "main": "index.js",
  "git repository": "https://github.com/massalabs/massa-proto-as.git",
  "devDependencies": {
    "as-proto": "^1.3.0",
    "as-proto-gen": "^1.3.0",
    "assemblyscript": "^0.27.5"
  },
  "scripts": {
    "test": "node tests",
    "asbuild:proto": "protoc --proto_path=massa-proto/proto/abis/ --proto_path=massa-proto/proto/commons/ --as_opt=gen-helper-methods --as_out=./assembly ./massa-proto/proto/abis/massa/abi/v1/abi.proto",
    "asbuild:index": "./generate_index-ts.sh > assembly/index.ts",
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release",
    "asbuild": "npm run asbuild:proto && npm run asbuild:index",
    "asbuild_windows": "npm run asbuild:proto && npm run asbuild_windows:index",
    "asbuild_windows:index": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./generate_index-ts.ps1 > assembly/index.ts",
    "start": "npx serve ."
  },
  "author": "",
  "license": "ISC",
  "description": "Generated AssemblyScrict source code from protobuf files for Massa blockchain.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  },
  "dependencies": {
    "binaryen": "^112.0.0-nightly.20230411",
    "fs-extra": "^10.1.0",
    "google-protobuf": "^3.21.2",
    "graceful-fs": "^4.2.11",
    "humps": "^2.0.1",
    "jsonfile": "^6.1.0",
    "long": "^5.2.3",
    "prettier": "^2.8.8",
    "universalify": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/massalabs/massa-proto-as.git"
  },
  "bugs": {
    "url": "https://github.com/massalabs/massa-proto-as/issues"
  },
  "homepage": "https://github.com/massalabs/massa-proto-as#readme"
}
